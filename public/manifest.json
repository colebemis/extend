{
  "manifest_version": 2,
  "name": "Extend",
  "version": "0.1.0",
  "description": "Manage your Chrome extensions from the toolbar",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "management"
  ],
  "content_security_policy":
    "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
